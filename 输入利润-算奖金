p1=eval(input("输入利润=（万元）"))
if p1<10:
    c1=p1*0.1
elif p1<=20:
    c1=10*0.1+(p1-10)*0.075
elif p1<=40:
    c1=10*0.1+(p1-10)*0.075+(p1-20)*0.05
elif p1<=60:
    c1=10*0.1+(p1-10)*0.075+(p1-20)*0.05+(p1-20)*0.03
elif p1<=100:
     c1=10*0.1+10*0.075+20*0.05+20*0.03+(p1-60)*0.015
elif p1>100:
     c1=10*0.1+10*0.075+20*0.05+20*0.03+40*0.015+(p1-100)*0.01
print(c1)
